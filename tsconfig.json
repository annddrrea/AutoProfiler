{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "*": ["./node_modules/@types/*", "*"]
    },
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "esnext",
    "types": [],
    "resolveJsonModule": true,
    "strict": false
  },
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": ["src/**/*.ts", "src/**/*.svelte"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
}
